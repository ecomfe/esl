{
  "name": "esl",
  "description": "ESL是一个浏览器端、符合AMD的标准加载器，适合用于现代Web浏览器端应用的入口与模块管理。",
  "version": "2.0.8",
  "contributors": [
    {
      "name": "errorrik",
      "email": "errorrik@gmail.com"
    }
  ],
  "repository": "git://github.com/ecomfe/esl",
  "scripts": {
    "pretest": "serve -L -p 8848 & echo $! > serve.pid && sleep 1",
    "test": "phantomjs test/run-phantom.js http://localhost:8848/test/test.html?autostart",
    "posttest": "kill `cat serve.pid` && rm serve.pid"
  },
  "devDependencies": {
    "phantomjs": "~1.9.7-1",
    "serve": "^1.4.0"
  }
}
